// showing default values
$contentWidth = 840px

.navbar {
  position: fixed !important;
}

table
  border-collapse collapse
  margin 1rem 0
  display: block
  overflow-x: auto

tr
  border-top 1px solid #dfe2e5

  &:nth-child(2n)
    background-color #fff

th, td
  border 1px solid #dfe2e5
  padding .6em 1em
